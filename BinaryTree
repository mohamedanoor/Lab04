
public class BinaryTree<E> {
protected Node<E> root;
// Nested Class for a tree node
protected static class Node<E> {
E data;
Node <E> left;
Node <E> right;
//node constructor
public Node(E data) {
this.data = data;
left = null;
right = null;
}
}

// BinaryTree constructor
public BinaryTree() {
root = null;
}
}

